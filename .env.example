# Environment Variables for MarryLocal App

# Crypto secret for encryption
VITE_CRYPTO_SECRET = "your-crypto-secret-here"

# Resend API key for email services
VITE_RESEND_API_KEY=your-resend-api-key-here

# Supabase configuration
VITE_SUPABASE_EDGE_FUNCTIONS_TOKEN=your-supabase-edge-functions-token-here
VITE_SUPABASE_KEY=your-supabase-anon-key-here
VITE_SUPABASE_URL=https://your-supabase-project.supabase.co

# Token encryption key
VITE_TOKEN_KEY = 'your-token-key-here'

# MarryLocal application URL (use localhost for dev, production URL for prod)
VITE_MARRYLOCAL_URL = 'https://marrylocal.in'
